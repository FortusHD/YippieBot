# Application Environment (dev or prod)
APP_ENV=dev

# Discord Bot Credentials - Development
BOT_TOKEN_DEV=your_dev_bot_token_here
BOT_CLIENT_ID_DEV=your_dev_client_id_here

# Discord Bot Credentials - Production
BOT_TOKEN_PROD=your_prod_bot_token_here
BOT_CLIENT_ID_PROD=your_prod_client_id_here

# Database
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_database_password
DB_ROOT_PASSWORD=your_database_root_password

# API Keys
GOOGLE_KEY=your_google_api_key_here

# Lavalink Configuration (for music functionality)
LAVALINK_HOST=your_lavalink_host
LAVALINK_PORT=2333
LAVALINK_PW=your_lavalink_password
LAVALINK_SECURE=false

# External Services
GIF_CREATOR=https://your-gif-creator-url.com/create_gif/

# Feature Flags
DEPLOY=false
TEST_WICHTELN=false

# Logging
LOG_LEVEL=DEBUG
ENABLE_ALERT=false